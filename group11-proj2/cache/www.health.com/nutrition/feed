    <media:content url="http://cdn-img.health.com/sites/default/files/styles/200x150/public/styles/main/public/gettyimages-517109570_0.jpg?itok=-mThBFZV" type="image/jpeg" isDefault="false" expression="sample" width="200" height="150" />
    <media:thumbnail url="http://cdn-img.health.com/sites/default/files/styles/75x75/public/styles/main/public/gettyimages-517109570_0.jpg?itok=ohEhCx0c" type="image/jpeg" isDefault="false" expression="sample" width="75" height="75">
    </media:thumbnail>
    <media:keywords/>
    <media:credit role="photographer"><p>Getty Images</p>
</media:credit>
  </media:group>
