    <media:thumbnail url="http://cdn-img.health.com/sites/default/files/styles/75x75/public/1474644359/GettyImages-536917684_0.jpg?itok=ScrbvptX" type="image/jpeg" isDefault="false" expression="sample" width="75" height="75">
