    <media:thumbnail url="http://cdn-img.health.com/sites/default/files/styles/75x75/public/1466541286/GettyImages-152831126_0.jpg?itok=jmh3ztiZ" type="image/jpeg" isDefault="false" expression="sample" width="75" height="75">
