    <media:thumbnail url="http://cdn-img.health.com/sites/default/files/styles/75x75/public/styles/main/public/gettyimages-479890534.jpg?itok=1KzHxrAk" type="image/jpeg" isDefault="false" expression="sample" width="75" height="75">
