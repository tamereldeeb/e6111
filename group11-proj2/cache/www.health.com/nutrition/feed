  <media:group>
    <media:content url="http://cdn-img.health.com/sites/default/files/styles/200x150/public/styles/main/public/gettyimages-154124817.jpg?itok=YNqBwgSC" type="image/jpeg" isDefault="false" expression="sample" width="200" height="150" />
    <media:thumbnail url="http://cdn-img.health.com/sites/default/files/styles/75x75/public/styles/main/public/gettyimages-154124817.jpg?itok=HjRgTzPB" type="image/jpeg" isDefault="false" expression="sample" width="75" height="75">
    </media:thumbnail>
    <media:keywords/>
    <media:credit role="photographer"><p>Getty Images</p>
