&media=http%3A%2F%2Fcdn-img.health.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2F2000x2000%2Fpublic%2F1463668621%2FGettyImages-521904983.jpg%3Fitok%3DuSUkOzn_" class="pinterest story-card__media-share" target="_blank" onclick="communityTrack('share-pinterest'); basicPopup(this.href); return false;">
